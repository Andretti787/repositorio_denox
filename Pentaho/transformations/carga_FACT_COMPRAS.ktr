    <sql>/* Famesa V8 */
SELECT CAB.FCY_0, (NETPRI_0 * QTYSTU_0) * CAB.SNS_0 AS IMP_TOT, ITMREF_0, ITMDES_0, NETPRI_0, QTYSTU_0, FORMAT(CAB.ACCDAT_0,'yyyyMMdd') AS FECHA, CAB.BPR_0 AS CODPROV, PAR.BPRNAM_0, CAB.NUM_0
FROM [192.168.28.200\X3FAMESA].x3famesa.FAMESAOF.PINVOICED LIN
INNER JOIN [192.168.28.200\X3FAMESA].x3famesa.FAMESAOF.PINVOICE CAB ON CAB.NUM_0 = LIN.NUM_0
INNER JOIN [192.168.28.200\X3FAMESA].x3famesa.FAMESAOF.BPARTNER PAR ON PAR.BPRNUM_0 = CAB.BPR_0
UNION ALL
/* Trilla V8 */
SELECT CAB.FCY_0, (NETPRI_0 * QTYSTU_0) * CAB.SNS_0 AS IMP_TOT, ITMREF_0, ITMDES_0, NETPRI_0, QTYSTU_0, FORMAT(CAB.ACCDAT_0,'yyyyMMdd') AS FECHA, CONCAT('T',CAB.BPR_0) AS CODPROV, PAR.BPRNAM_0, CAB.NUM_0
FROM [192.168.28.200\X3FAMESA].x3famesa.TRILLA.PINVOICED LIN
INNER JOIN [192.168.28.200\X3FAMESA].x3famesa.TRILLA.PINVOICE CAB ON CAB.NUM_0 = LIN.NUM_0
INNER JOIN [192.168.28.200\X3FAMESA].x3famesa.TRILLA.BPARTNER PAR ON PAR.BPRNUM_0 = CAB.BPR_0
UNION ALL 
/* V12 */
SELECT CAB.FCY_0 Collate Latin1_General_BIN2, (NETPRI_0 * QTYSTU_0) * CAB.SNS_0 AS IMP_TOT, ITMREF_0 Collate Latin1_General_BIN2, ITMDES_0 Collate Latin1_General_BIN2, NETPRI_0, QTYSTU_0, FORMAT(CAB.ACCDAT_0,'yyyyMMdd') AS FECHA
, CAB.BPR_0 Collate Latin1_General_BIN2 AS CODPROV, PAR.BPRNAM_0 Collate Latin1_General_BIN2, CAB.NUM_0 Collate Latin1_General_BIN2
FROM LIVE.PINVOICED LIN
INNER JOIN LIVE.PINVOICE CAB ON CAB.NUM_0 = LIN.NUM_0
INNER JOIN LIVE.BPARTNER PAR ON PAR.BPRNUM_0 = CAB.BPR_0</sql>
