    <sql>SELECT MFG.PLNFCY_0, MFG.MFGNUM_0, MFG.ROUNUM_0, ITM.ITMREF_0, ITMDES1_0, MFI.LOT_0, ITM.EANCOD_0, ITM.YEANCOD14_0
, CAST(ROUND(MFG.EXTQTY_0 * 1.02, 0) AS INT) AS CANT
fROM LIVE.MFGHEAD MFG
INNER JOIN LIVE.MFGITM MFI ON MFI.MFGNUM_0 = MFG.MFGNUM_0
INNER JOIN LIVE.ITMMASTER ITM ON ITM.ITMREF_0 = MFG.ROUNUM_0
WHERE ITM.EANCOD_0 != ''</sql>
