    <connection>MSSQL v8</connection>
    <sql>SELECT CAB.SIVTYP_0, CAB.NUM_0, CAB.BPR_0, CAB.BPRSAC_0, CAB.FCY_0, CAB.ACCDAT_0, CAB.CUR_0, CAB.PTE_0, CAB.VAC_0, CAB.BPRVCR_0  
, GAC.DUDLIG_0, GAC.DUDDAT_0, GAC.PAM_0, CAST(GAC.AMTCUR_0 AS DECIMAL(10,2)) AS AMTCUR
, GAC.ACCNUM_0, CAST((SUM(GAC.AMTCUR_0) - SUM(GAC.PAYCUR_0) -SUM(GAC.TMPCUR_0)) AS DECIMAL(10,2))AS PDTE
, GROUPING (CAB.NUM_0) AS GRUPING_NUM
FROM TRILLA.GACCDUDATE GAC
INNER JOIN TRILLA.SINVOICE CAB ON CAB.NUM_0 = GAC.NUM_0
WHERE AMTCUR_0 - PAYCUR_0 - TMPCUR_0 > 0
AND DUDDAT_0 >= '20210625'
AND SIVTYP_0 IN ('FAC','ABC')
GROUP BY 
		GROUPING SETS (
						(CAB.SIVTYP_0,	CAB.NUM_0,	CAB.BPR_0,	CAB.BPRSAC_0, 	CAB.FCY_0,	CAB.ACCDAT_0,	CAB.CUR_0,	CAB.PTE_0,	CAB.VAC_0,	CAB.BPRVCR_0,	CAB.BPRVCR_0, GAC.ACCNUM_0,GAC.DUDLIG_0, GAC.DUDDAT_0, GAC.PAM_0, GAC.AMTCUR_0),
						()
						)
</sql>
