    <connection>MSSQL v8</connection>
    <sql>SELECT 'TRA' AS TIPO_ASIENTO, NULL AS NUMDOC, 'FAM' AS PLANTA, '20211231' AS FEC_CONTABLE, 'Movimientos acumulados Dic-2021' AS DESCRIP_CAB, 'EUR' AS DIVISA
, ROW_NUMBER() OVER (ORDER BY FIN.CUENTA) AS ROWNUM , 1 AS TIPO_REFERENCIAL, CASE WHEN FIN.CUENTA IN ('400000','403000') 
																				THEN '400999' 
																				ELSE  CASE WHEN (FIN.CUENTA) IN ('430000','433000') THEN '430999' 
																						ELSE FIN.CUENTA 
																						END 
																				END AS CUENTA
																
, 'Movimientos acumulados Dic-2021' AS DECRIP_LIN
, CASE WHEN (SUM(FIN.IMPORTE_ASIENTO)) &lt; 0 THEN -1
		ELSE 1							
		END AS SIGNO
,  ABS(CAST(SUM(FIN.IMPORTE_ASIENTO) AS DECIMAL(10,2))) AS IMPORTE,  FIN.TASA_IMPUESTO
, CASE (ROW_NUMBER() OVER (ORDER BY FIN.CUENTA)) WHEN 1 THEN 'CAB'
	ELSE ''
	END AS MARCA
FROM 
	(
	SELECT --ROW_NUMBER() OVER (ORDER BY DATOS.ACC_0,SNS_0) AS ROWNUM
	DATOS.ACC_0 AS CUENTA, DATOS.SNS_0 * SUM(DATOS.AMTCUR_0) AS IMPORTE_ASIENTO
	, DATOS.TASA_IMPUESTO
	FROM 
		(
		SELECT TYP_0, NUM_0, LIN_0, LEDTYP_0,	ACC_0,	DES_0,	SNS_0,	AMTCUR_0, AMTLED_0
		, CASE WHEN SUBSTRING(ACC_0,1,1) IN ('6','7') THEN 'SP999'
			WHEN SUBSTRING(ACC_0,1,2) IN ('20','21') THEN 'SP999'
			WHEN ACC_0 IN('472000','477000') THEN 'SP999'
			ELSE ''
			END AS TASA_IMPUESTO
		FROM FAMESAOF.GACCENTRYD
--		WHERE FIY_0 IN (4,5,6)
		WHERE LEDTYP_0 = 1
		) AS DATOS
--WHERE TYP_0 IN ('FAC','ABC')
	GROUP BY DATOS.ACC_0, DATOS.SNS_0, DATOS.TASA_IMPUESTO
	) AS FIN
GROUP BY FIN.CUENTA, FIN.TASA_IMPUESTO
UNION ALL
