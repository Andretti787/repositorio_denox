    <sql>SELECT CUS.BPSNUM_0 , CUS.BPSNAM_0, CUS.BPSGRU_0
, CUS.VACBPR_0, CUS.TSSCOD_0, CUS.TSSCOD_1, CUS.TSSCOD_2, CUS.TSSCOD_3, CUS.TSSCOD_4
, PAR.CRY_0 
, CASE WHEN ISNULL(V12F.CREDATTIM_0,'') = '' THEN  (CASE WHEN ISNULL(V12T.CREDATTIM_0,'') = '' THEN CUS.CREDATTIM_0
															ELSE V12T.CREDATTIM_0
															END)
			ELSE V12F.CREDATTIM_0
			END AS FEC_CREACION
						
FROM LIVE.BPSUPPLIER CUS
LEFT JOIN LIVE.BPARTNER PAR ON PAR.BPRNUM_0 = CUS.BPCNUM_0
LEFT JOIN [192.168.28.200\X3FAMESA].x3famesa.FAMESAOF.BPSUPPLIER V12F ON V12F.BPSNUM_0 = CUS.BPSNUM_0 COLLATE Latin1_General_BIN2
LEFT JOIN [192.168.28.200\X3FAMESA].x3famesa.TRILLA.BPSUPPLIER V12T ON CONCAT('T', V12T.BPSNUM_0 ) = CUS.BPSNUM_0 COLLATE Latin1_General_BIN2</sql>
