    <connection>MSSQL SAGE</connection>
    <sql>SELECT CUS.BPCNUM_0, CUS.BPCNAM_0, CUS.BPCGRU_0, CUS.BPCSTA_0
, CUS.REP_0, CUS.REP_1, CUS.VACBPR_0, CUS.TSCCOD_0, CUS.TSCCOD_1, CUS.TSCCOD_2, CUS.TSCCOD_3, CUS.TSCCOD_4
, PAR.CRY_0 
, CASE WHEN ISNULL(V12F.CREDATTIM_0,'') = '' THEN  (CASE WHEN ISNULL(V12T.CREDATTIM_0,'') = '' THEN CUS.CREDATTIM_0
															ELSE V12T.CREDATTIM_0
															END)
			ELSE V12F.CREDATTIM_0
			END AS FEC_CREACION
						
FROM LIVE.BPCUSTOMER CUS
LEFT JOIN LIVE.BPARTNER PAR ON PAR.BPRNUM_0 = CUS.BPCNUM_0
LEFT JOIN [192.168.28.200\X3FAMESA].x3famesa.FAMESAOF.BPCUSTOMER V12F ON V12F.BPCNUM_0 = CUS.BPCNUM_0 COLLATE Latin1_General_BIN2
LEFT JOIN [192.168.28.200\X3FAMESA].x3famesa.TRILLA.BPCUSTOMER V12T ON CONCAT('T', V12T.BPCNUM_0 ) = CUS.BPCNUM_0 COLLATE Latin1_General_BIN2
</sql>
