    <connection>MSSQL V8</connection>
    <sql>WITH RECUR AS
(
select TSICOD_1, 'E' as ID_CAB, 19 AS TIPO_REG, '' AS NUM_ASIENTO, 'TRI' AS PLANTA, FORMAT(GETDATE(), 'yyyyMMdd') AS FECHA, 'PROYECTO' AS PROJECT, '' AS COD_MOV
, 'L' AS ID_LIN, '' AS NUM_LIN, STO.ITMREF_0, ITM.ITMDES1_0 AS DESCRIP, STO.PCU_0 AS UD, STO.QTYSTU_0 AS CANT, STO.PCU_0 AS UD_STOCK
, STO.QTYSTU_0 AS CANT_US
, CASE (TSICOD_1) WHEN '20' THEN 
					(SELECT VLTTOT_0
							FROM TRILLA.ITMCOST IT2
							WHERE ITMREF_0 = STO.ITMREF_0
							AND ITCSEQ_0 = (SELECT MAX(ITCSEQ_0)
											FROM TRILLA.ITMCOST COST
											WHERE COST.ITMREF_0 = IT2.ITMREF_0
											AND CSTTYP_0 = 1) 
					AND CSTTYP_0 = 1 --PRECIO ESTÁNDAR 
					)
				  ELSE 
					ITC.VLTTOT_0 
				  END AS VLTTOT_0
, 'S' AS ID_DET, STO.PCU_0 AS UD_DET, STO.QTYSTU_0 AS CANT_DET, STO.BPSLOT_0
, CASE  WHEN (TSICOD_1) IN ('20','22')
						THEN TRILLA.ZDECODE (LOT_0, '', CONCAT('T','22',LEFT(STO.LOC_0,3)), LOT_0)
						ELSE LOT_0 END AS LOTE_CALCULADO
, CASE WHEN (TSICOD_1) IN ('20','22')
						THEN TRILLA.ZDECODE (SLO_0, '', '000', SLO_0)
						ELSE LOT_0 END AS SUBLOTE_CALCULADO
, CASE (ITF.PCKCAP_0) WHEN 0 THEN 1 ELSE ITF.PCKCAP_0 END AS CAP_PALLET
--, CEILING( STO.QTYSTU_0 / CASE ITF.PCKCAP_0 WHEN 0 THEN 1 ELSE ITF.PCKCAP_0 END) AS NUM_PALLET
, CEILING( STO.QTYSTU_0 / CASE ITF.PCKCAP_0 WHEN 0 THEN  STO.QTYSTU_0 / 3 ELSE ITF.PCKCAP_0 END) AS NUM_PALLET
, CAST (STO.QTYSTU_0 % CASE ITF.PCKCAP_0 WHEN 0 THEN 1 ELSE ITF.PCKCAP_0 END AS INT)  AS RESTO
, 1 AS T
, STO.LOC_0 AS UBICACION, STO.STA_0 AS ESTADO
From TRILLA.STOCK STO 
LEFT join TRILLA.ZV_ARTCOST ITC ON ITC.ITMREF_0 = STO.ITMREF_0
LEFT JOIN TRILLA.ITMMASTER ITM ON ITM.ITMREF_0 = STO.ITMREF_0
LEFT JOIN TRILLA.ITMFACILIT ITF ON ITF.ITMREF_0 = STO.ITMREF_0
WHERE STO.ITMREF_0 NOT IN  ('236030','236093','236500','236990','236200')
AND STO.ITMREF_0 NOT IN (SELECT ITMREF_0  COLLATE Latin1_General_BIN2 FROM [192.168.28.100\X3V12].x3.LIVE.STOCK WHERE STOFCY_0 = 'TRI')
--where STO.ITMREF_0 IN ('16010030','16061030')
UNION ALL
SELECT TSICOD_1, ID_CAB, TIPO_REG, NUM_ASIENTO, PLANTA, FECHA, PROJECT, COD_MOV, ID_LIN, NUM_LIN, ITMREF_0, DESCRIP, UD, CANT, UD_STOCK
, CANT_US, VLTTOT_0
, ID_DET, UD_DET, CANT_DET, BPSLOT_0, LOTE_CALCULADO, SUBLOTE_CALCULADO, CAP_PALLET, NUM_PALLET
, RESTO, T+1 AS T, UBICACION, ESTADO
FROM RECUR WHERE T&lt; NUM_PALLET
AND TSICOD_1 in ('20','22')--SÓLO CALCULO SUBLOTES PARA EL CANAL 20
) 
SELECT TSICOD_1, ID_CAB, TIPO_REG, NUM_ASIENTO, PLANTA, FECHA, PROJECT, COD_MOV, ID_LIN, NUM_LIN, ITMREF_0, DESCRIP, UD, CANT, UD_STOCK
, CANT_US, VLTTOT_0
, ID_DET, UD_DET, CANT_DET, BPSLOT_0, LOTE_CALCULADO, NUM_PALLET 
, CASE WHEN (SUBLOTE_CALCULADO&lt;&gt;'') THEN RIGHT('00000' + Ltrim(Rtrim(CONCAT(SUBLOTE_CALCULADO, CONVERT(VARCHAR(3), T)))),5) ELSE SUBLOTE_CALCULADO END AS SUBLOTE_CALCULADO
, CAP_PALLET, NUM_PALLET, RESTO, T, UBICACION, ESTADO
, CASE WHEN (TSICOD_1) IN ('20','22') THEN 
									(
										CASE WHEN NUM_PALLET &lt;&gt; T THEN CAP_PALLET
																	ELSE RESTO
																	END 
									) 
									ELSE CANT_DET
									END AS CANTIDAD_SUBLOTE
FROM RECUR

ORDER BY TSICOD_1, ITMREF_0, UBICACION, LOTE_CALCULADO, T
OPTION(MAXRECURSION 1000);</sql>
