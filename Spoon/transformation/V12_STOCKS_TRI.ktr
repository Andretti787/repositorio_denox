    <connection>MSSQL V8</connection>
    <sql>select 'E' AS ID_CAB, 19 as TIPO_REG, '' AS NUM_ASIENTO, 'TRI' AS PLANTA,  '20211230' AS FECHA, 'PROYECTO' AS PROJECT, '' AS COD_MOV
, 'L' AS ID_LIN, '' AS NUM_LIN, STO.ITMREF_0, ITM.ITMDES1_0 AS DESCRIP, STO.PCU_0 UD, STO.QTYSTU_0 as CANT, STO.PCU_0 AS UD_STOCK, STO.QTYSTU_0 AS CANT_US
, CASE (TSICOD_1) WHEN '20' THEN 
					(SELECT VLTTOT_0 FROM TRILLA.ZV_ARTCOST 
					WHERE ITMREF_0 = STO.ITMREF_0
					AND CSTTYP_0 = 2 -- PRECIO ESTÁNDAR ACTUALIZADO
					)
				  ELSE 
					ITC.VLTTOT_0 
				  END AS VLTTOT_0
, 'S' AS ID_DET, STO.PCU_0 AS UD_DET, STO.QTYSTU_0 AS CANT_DET
, CASE (TSICOD_1) WHEN '20' 
						THEN TRILLA.ZDECODE (LOT_0, '', 'T220101', LOT_0)
						ELSE LOT_0 END AS LOTE_CALCULADO
, CASE (TSICOD_1) WHEN '20' 
						THEN TRILLA.ZDECODE (SLO_0, '', '00001', SLO_0)
						ELSE LOT_0 END AS SUBLOTE_CALCULADO
, STO.LOC_0 AS UBICACION, STO.STA_0 AS ESTADO
From TRILLA.STOCK STO 
LEFT join TRILLA.ZV_ARTCOST ITC ON ITC.ITMREF_0 = STO.ITMREF_0
LEFT JOIN TRILLA.ITMMASTER ITM ON ITM.ITMREF_0 = STO.ITMREF_0
</sql>
