    <sql>SELECT CAB.NUM_0, CAB.PAYTYP_0, CAB.UMRNUM_0, CAB.BPR_0, CAB.COA_0 AS CODIGO_PLAN_CAB, CAB.ACC_0 AS CUENTA_CAB, CAB.CHQNUM_0, CAB.CHQBAN_0, CAB.PAM_0, CAB.FCY_0, CAB.BAN_0, CAB.SNS_0, CAB.CUR_0
, CAB.AMTCUR_0, CAB.AMTBAN_0, 0 AS 'CAMBIO DIVISA', CAB.ACCDAT_0, CAB.DUDDAT_0, CAB.STA_0  
, DET.DENCOD_0, DET.COA_0 AS CODIGO_PLAN_LIN, DET.ACC_0 AS CUENTA_LIN, DET.BPRLIN_0, DET.VCRTYP_0, DET.VCRNUM_0, DET.CURLIN_0, DET.AMTLIN_0, DET.VATLIN_0
, DET.LIN_0 
FROM FAMESAOF.PAYMENTH CAB 
INNER JOIN FAMESAOF.PAYMENTD DET ON CAB.NUM_0 = DET.NUM_0
WHERE CAB.STA_0 = 1 --1- ESTADO TECLEADO, 9-EN BANCO
--AND PAYTYP_0 = 'CHQRD' --CHEQUE RECIBIDO REMESA OR 'PPAG' --PAGARÉ EMITIDO
UNION ALL
SELECT CAB.NUM_0, CAB.PAYTYP_0, CAB.UMRNUM_0, CAB.BPR_0, CAB.COA_0 AS CODIGO_PLAN_CAB, CAB.ACC_0 AS CUENTA_CAB, CAB.CHQNUM_0, CAB.CHQBAN_0, CAB.PAM_0, CAB.FCY_0, CAB.BAN_0, CAB.SNS_0, CAB.CUR_0
, CAB.AMTCUR_0, CAB.AMTBAN_0, 0 AS 'CAMBIO DIVISA', CAB.ACCDAT_0, CAB.DUDDAT_0, CAB.STA_0  
, DET.DENCOD_0, DET.COA_0 AS CODIGO_PLAN_LIN, DET.ACC_0 AS CUENTA_LIN, DET.BPRLIN_0, DET.VCRTYP_0, DET.VCRNUM_0, DET.CURLIN_0, DET.AMTLIN_0, DET.VATLIN_0
, DET.LIN_0
FROM TRILLA.PAYMENTH CAB 
INNER JOIN TRILLA.PAYMENTD DET ON CAB.NUM_0 = DET.NUM_0
WHERE CAB.STA_0 = 1 --1- ESTADO TECLEADO, 9-EN BANCO
--AND PAYTYP_0 = 'CHQRD' --CHEQUE RECIBIDO REMESA OR 'PPAG' --PAGARÉ EMITIDO
UNION ALL
SELECT CAB.NUM_0, CAB.PAYTYP_0, CAB.UMRNUM_0, CAB.BPR_0, CAB.COA_0 AS CODIGO_PLAN_CAB, CAB.ACC_0 AS CUENTA_CAB, CAB.CHQNUM_0, CAB.CHQBAN_0, CAB.PAM_0, CAB.FCY_0, CAB.BAN_0, CAB.SNS_0, CAB.CUR_0
, CAB.AMTCUR_0, CAB.AMTBAN_0, 0 AS 'CAMBIO DIVISA', CAB.ACCDAT_0, CAB.DUDDAT_0, CAB.STA_0  
, DET.DENCOD_0, DET.COA_0 AS CODIGO_PLAN_LIN, DET.ACC_0 AS CUENTA_LIN, DET.BPRLIN_0, DET.VCRTYP_0, DET.VCRNUM_0, DET.CURLIN_0, DET.AMTLIN_0, DET.VATLIN_0
, DET.LIN_0
FROM IPTFAM.PAYMENTH CAB 
INNER JOIN IPTFAM.PAYMENTD DET ON CAB.NUM_0 = DET.NUM_0
WHERE CAB.STA_0 = 1 --1- ESTADO TECLEADO, 9-EN BANCO
--AND PAYTYP_0 = 'CHQRD' --CHEQUE RECIBIDO REMESA OR 'PPAG' --PAGARÉ EMITIDO
UNION ALL
SELECT CAB.NUM_0, CAB.PAYTYP_0, CAB.UMRNUM_0, CAB.BPR_0, CAB.COA_0 AS CODIGO_PLAN_CAB, CAB.ACC_0 AS CUENTA_CAB, CAB.CHQNUM_0, CAB.CHQBAN_0, CAB.PAM_0, CAB.FCY_0, CAB.BAN_0, CAB.SNS_0, CAB.CUR_0
, CAB.AMTCUR_0, CAB.AMTBAN_0, 0 AS 'CAMBIO DIVISA', CAB.ACCDAT_0, CAB.DUDDAT_0, CAB.STA_0  
, DET.DENCOD_0, DET.COA_0 AS CODIGO_PLAN_LIN, DET.ACC_0 AS CUENTA_LIN, DET.BPRLIN_0, DET.VCRTYP_0, DET.VCRNUM_0, DET.CURLIN_0, DET.AMTLIN_0, DET.VATLIN_0
, DET.LIN_0
FROM IPTRILLA.PAYMENTH CAB 
INNER JOIN IPTRILLA.PAYMENTD DET ON CAB.NUM_0 = DET.NUM_0
WHERE CAB.STA_0 = 1 --1- ESTADO TECLEADO, 9-EN BANCO
--AND PAYTYP_0 = 'CHQRD' --CHEQUE RECIBIDO REMESA OR 'PPAG' --PAGARÉ EMITIDO</sql>
